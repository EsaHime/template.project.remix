{
  "name": "remix-template-remix",
  "private": true,
  "description": "- [Remix Docs](https://remix.run/docs)",
  "license": "ISC",
  "sideEffects": false,
  "scripts": {
    "build": "dotenv -e .env remix build",
    "dev": "dotenv -e .env.development remix dev",
    "start": "dotenv -e .env remix-serve build",
    "prisma:dev-migrate": "dotenv -e .env.development prisma migrate dev",
    "prisma:dev-generate": "dotenv -e .env.development prisma generate",
    "prisma:dev-seed": "dotenv -e .env.development node bin/seed.mjs"
  },
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "@remix-run/node": "^1.3.4",
    "@remix-run/react": "^1.3.4",
    "@remix-run/serve": "^1.3.4",
    "dotenv-cli": "^5.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.4",
    "@remix-run/eslint-config": "^1.3.4",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "prisma": "^3.11.1",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  },
  "version": "1.0.0",
  "main": ".eslintrc.js",
  "keywords": [],
  "author": ""
}
